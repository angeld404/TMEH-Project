#include "msp.h"
#include<string.h>
#include "set_dco.h"
#include "delay_us.h"
#include "LCD.h"

/**
 * Jonathan Lau, Angel Delgado
 * CPE 329 Spring 2019
 * Professor Hummel
 *
 * Assignment 2:  Blinking LED, Clock Control, and Software Delay
 */

void main(void) {
    // stop watchdog timer
    WDT_A->CTL = WDT_A_CTL_PW | WDT_A_CTL_HOLD;

    //set MCLK to DCO at nominal 48 MHz
    set_DCO(FREQ_6_MHZ);

    //blink phrase "Hello World!!!!!!!!!!!!!" indefinitely
    while(1){
        LCD_Instr(LCD_DISP_CLEAR);
        delay_us(700000);
        Write_string_LCD("Hello World!!!!!!!!!!!!!");
        delay_us(700000);
    }

 }


